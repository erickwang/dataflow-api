{
  "name": "dataflow-api",
  "version": "0.0.1",
  "description": "JavaScript API for dataflow processing",
  "keywords": [
    "vega",
    "dataflow",
    "aggregate",
    "filter",
    "sort"
  ],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jeffrey Heer",
    "url": "http://idl.cs.washington.edu"
  },
  "main": "build/dataflow-api.js",
  "module": "index",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/vega/dataflow-api.git"
  },
  "scripts": {
    "build": "npm run test",
    "postbuild": "uglifyjs build/dataflow-api.js -c -m -o build/dataflow-api.min.js",
    "pretest": "rm -rf build && mkdir build && rollup -g d3-array:d3,vega-dataflow:vega,vega-transforms:vega.transforms,vega-statistics:vega,vega-util:vega -f umd -n df -o build/dataflow-api.js -- index.js",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test",
    "prepublish": "npm run build",
    "postpublish": "git push && git push --tags && zip -j build/dataflow-api.zip -- LICENSE README.md build/dataflow-api.js"
  },
  "dependencies": {
    "vega-dataflow": "3",
    "vega-transforms": "1",
    "vega-util": "1"
  },
  "devDependencies": {
    "eslint": "4",
    "rollup": "0.43",
    "rollup-plugin-node-resolve": "3",
    "tape": "4",
    "uglify-es": "3"
  }
}
